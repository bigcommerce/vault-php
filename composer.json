{
    "name": "serpentblade/php-vault",
    "description": "HashiCorp Vault HTTP Client",
    "type": "library",
    "keywords": ["hashicorp", "vault", "security", "password"],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "~6.0 || ~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Krieger",
            "email": "dan@kriegertek.com"
        }
    ],
    "suggest": {
        "ext-curl": "Send HTTP requests via cURL",
        "ext-openssl": "Secure requests to Vault"
    },
    "autoload": {
        "psr-4": {
            "VaultPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VaultPhp\\Test\\": "tests/"
        }
    }
}
